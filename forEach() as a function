// Write your code here:

function greetAliens(strArr) {
  for (let i =0; i < strArr.length; i++) {
    let alienName = strArr[i];
    console.log(`Oh powerful ${alienName}, we humans offer our unconditional surrender!`);
  }
}; //function that performs similar operation as .forEach() method.







// When you're ready to test your code, uncomment the below and run:

const aliens = ["Blorgous", "Glamyx", "Wegord", "SpaceKing"];

greetAliens(aliens);
