const groceryList = ['orange juice', 'bananas', 'coffee beans', 'brown rice', 'pasta', 'coconut oil', 'plantains'];

groceryList.shift(); //removed orange juice from the front
console.log(groceryList);

groceryList.unshift('popcorn'); //added popcorn to beginnging of array
console.log(groceryList);

console.log(groceryList.slice(1, 4)); //only logs to console the first 3 items in the array.

console.log(groceryList.slice(1, 4));

const pastaIndex = groceryList.indexOf('pasta');

console.log(pastaIndex); //only logs the index of the 'pasta' element.
