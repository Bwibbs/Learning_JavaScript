let capFave = spaceship.crew.captain['favorite foods'][0];//creates a new variable called capFave and assigns captains favorite food in the 0th index to it
spaceship.passengers = [{name: 'Rex'}]; //changes the value of passengers property.
let firstPassenger = spaceship.passengers[0];//creates variable firstPassenger and assigns the oth index of the the spaceship.passengers to it
