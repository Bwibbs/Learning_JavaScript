// Write your function here:
function sillySentence (string1, string2, string3) {
  return `I am so ${string1} because I ${string2} coding! Time to write some more awesome ${string3}`
}

// Uncomment the line below when you're ready to try out your function
console.log(sillySentence('excited', 'love', 'functions')) // Should return 'I am so excited because I love coding! Time to write some more awesome functions!'
