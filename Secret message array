let secretMessage = ['Learning', 'is', 'not', 'about', 'what', 'you', 'get', 'easily', 'the', 'first', 'time,', 'it', 'is', 'about', 'what', 'you', 'can', 'figure', 'out.', '-2015,', 'Chris', 'Pine,', 'Learn', 'JavaScript'];
secretMessage.pop(); //remove 'JavaScript' from secretMessage array.
console.log(secretMessage.length); //shows the current length of the secretMessage array.
secretMessage.push('to', 'Program')// add 2 new elements to the end of secretMessage array.
secretMessage[7] = 'right'; //changes element 7 to 'right'
secretMessage.shift(); //remove 'Learning' from array.
secretMessage.unshift('Programming'); //add 'Programming' to the beginning of the array.
secretMessage.splice(6, 5, 'know'); //use the .splice() method to start at element 6 of the array and remove the next 5 elements and replace them with the string 'know'.
console.log(secretMessage.join());
