console.log(Math.random()*100); //generates a random number and multiplies it by 100

console.log(Math.floor(Math.random())*100);//generates a random number, multiplies it by 100, and then rounds down to the nearest whole number

console.log(Math.ceil(43.8));//Returns the smallest integer greater than or equal to 43.8

console.log(Number.isInteger(2017));//method that checks if number is an integer
