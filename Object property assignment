let spaceship = {
  'Fuel Type' : 'Turbo Fuel',
  homePlanet : 'Earth',
  color: 'silver',
  'Secret Mission' : 'Discover life outside of Earth.'
};

// Write your code below
spaceship.color = 'glorious gold'; //reassigns color property
spaceship.numEngines = 4;//add property called numEngines
delete spaceship['Secret Mission'];//removes property from spaceship object
