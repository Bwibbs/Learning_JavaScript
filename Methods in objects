let retreatMessage = 'We no longer wish to conquer your planet. It is full of dogs, which we do not care for.';

// object that has two methods called retreat and takeOff
let alienShip = {
  retreat() {
    console.log(retreatMessage)
  },
  takeOff() {
    console.log('Spim... Borp... Glix... Blastoff!')
  }
};

alienShip.retreat(); //invoke the retreat method
alienShip.takeOff(); //invoke the takeOff method
